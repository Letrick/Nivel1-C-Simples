#include<iostream>

int main () {
	
	int numero;
	
	std::cout << "Digite um numero e saberemos se e PAR ou IMPAR: ";
	std::cin  >> numero;
	
	
	if(numero % 2 == 0){
	std::cout << "Par";
		
		
	}
	else{
	std::cout << "Impar";
		
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
