#include<iostream>


int main(){
	
	int numero1, numero2, resultado;
	
	
	std::cout << "Digite o primeiro numero para somar: ";
	std::cin  >> numero1;
	
	std::cout << "Digite o segundo numero para somar: ";
	std::cin  >> numero2;
	
	resultado = numero1 + numero2;
	if(resultado > 10){
	std::cout << "Resultado da soma: " << resultado;
	}
	
	else{
		
		std::cout << "errou ";
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
